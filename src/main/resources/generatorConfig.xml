        <javaModelGenerator targetPackage="com.bl.bootcamp.msproductos.persistencia.modelo" targetProject="src/main/java" />
        <sqlMapGenerator targetPackage="com.bl.bootcamp.msproductos.persistencia.mapper.sqlmap" targetProject="src/main/resources" />
        <javaClientGenerator targetPackage="com.bl.bootcamp.msproductos.persistencia.mapper" targetProject="src/main/java" type="XMLMAPPER" />

        <table schema="producto" tableName="producto" domainObjectName="ProductoBean" enableCountByExample="false" enableDeleteByExample="false" enableUpdateByExample="false" selectByPrimaryKeyQueryId="false" selectByExampleQueryId="false" enableSelectByExample="false" modelType="flat" />


